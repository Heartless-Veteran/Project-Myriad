<?xml version="1.0" encoding="UTF-8"?>
<!--
    OWASP Dependency Check Suppressions for Project Myriad
    This file contains suppressions for known false positives or accepted vulnerabilities
-->
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    
    <!-- Suppress false positives for Android framework -->
    <suppress>
        <notes><![CDATA[
            Android framework components - these are managed by the Android platform
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/androidx\..*</packageUrl>
        <cve>CVE-2020-15522</cve>
    </suppress>
    
    <!-- Suppress false positives for Kotlin standard library -->
    <suppress>
        <notes><![CDATA[
            Kotlin standard library false positives
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.jetbrains\.kotlin/kotlin-stdlib.*</packageUrl>
        <cve>CVE-2019-10101</cve>
    </suppress>
    
    <!-- Add specific suppressions as needed for legitimate dependencies -->
    
</suppressions>